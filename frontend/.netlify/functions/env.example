# Configurações DocuSign
DOCUSIGN_API_BASE_URL=https://demo.docusign.net/restapi
DOCUSIGN_ACCOUNT_ID=your_account_id_here
DOCUSIGN_USER_ID=your_user_id_here
DOCUSIGN_INTEGRATION_KEY=your_integration_key_here
DOCUSIGN_RSA_PRIVATE_KEY=your_rsa_private_key_here
DOCUSIGN_OAUTH_BASE_PATH=account-d.docusign.com

# Configurações de Logging
LOG_LEVEL=info
ENABLE_AUDIT_LOG=true
LOG_DOCUSIGN_REQUESTS=true

# Configurações de Segurança
ENABLE_RATE_LIMITING=true
MAX_REQUESTS_PER_MINUTE=100
ENABLE_CORS=true
ALLOWED_ORIGINS=https://crispy-octo-spoon.netlify.app,https://*.netlify.app

# Configurações do Netlify
NODE_ENV=production

# Configurações do Supabase (para as funções de clientes)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Configurações de Cache e Logging
ENABLE_CACHE=false
CACHE_TTL=300
ENABLE_QUERY_LOG=true
